<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox minHeight="40.0" prefWidth="585.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.settings.AccountTab">
   <children>
      <AnchorPane maxHeight="-Infinity" minHeight="-Infinity" prefHeight="50.0" prefWidth="200.0" style="-fx-background-color: LIGHTSALMON;">
         <children>
            <Label layoutX="14.0" layoutY="12.0" text="Account Settings" textFill="WHITE" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0">
               <font>
                  <Font name="Berlin Sans FB" size="24.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <AnchorPane prefHeight="123.0" prefWidth="585.0">
         <children>
            <PasswordField fx:id="oldPassword" layoutX="39.0" layoutY="31.0" promptText="Old Password" AnchorPane.leftAnchor="40.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="40.0" />
            <PasswordField fx:id="newPassword" layoutX="68.0" layoutY="88.0" promptText="New Password" AnchorPane.leftAnchor="40.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="80.0" />
            <PasswordField fx:id="newConfirm" layoutX="-3.0" layoutY="104.0" promptText="Confirm New Password" AnchorPane.leftAnchor="40.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="120.0" />
         </children>
      </AnchorPane>
      <AnchorPane prefHeight="200.0" prefWidth="585.0" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="reset" layoutX="43.0" layoutY="41.0" mnemonicParsing="false" text="Reset Password" AnchorPane.leftAnchor="40.0" AnchorPane.topAnchor="41.0" />
            <Button fx:id="logout" layoutX="39.0" layoutY="100.0" mnemonicParsing="false" text="Logout" AnchorPane.leftAnchor="40.0" AnchorPane.topAnchor="100.0" />
            <Label fx:id="info" layoutX="40.0" layoutY="14.0" text="Initial Text" />
         </children>
      </AnchorPane>
   </children>
</VBox>
